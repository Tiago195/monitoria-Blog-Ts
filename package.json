{
  "name": "blogs_api_typescript",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "db:drop": "sequelize-cli db:drop",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "npm run build && sequelize-cli db:seed:all",
    "db:reset": " npm run build && npm run db:drop && npm run db:create && npm run db:migrate",
    "build": "npx tsc",
    "start:test": "API_PORT=3030 NODE_ENV=test npm start",
    "kill:test": "if lsof -t -i:3030; then kill -9 $(lsof -t -i:3030); fi",
    "pretest": "(npm run kill:test && NODE_ENV=test npm run start:test) &",
    "test": "npx -y tsc && npx -y wait-on -t 300000 http://localhost:3030 && NODE_ENV=test API_PORT=3030 jest -i --verbose --forceExit",
    "test:dev": "npm run build && npx -y wait-on -t 300000 http://localhost:3000 && NODE_ENV=development API_PORT=3000 jest -i --verbose --forceExit",
    "posttest": "npm run run kill:test",
    "test-coverage": "jest -i --verbose --forceExit --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "8.2.0",
    "express": "^4.18.2",
    "express-async-errors": "3.1.1",
    "joi": "17.6.0",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.10.0",
    "mysql2": "2.1.0",
    "sequelize": "6.3.4",
    "shelljs": "0.8.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "change-case": "4.1.2",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "frisby": "2.1.2",
    "jest": "26.0.1",
    "jest-each": "28.0.1",
    "nodemon": "^2.0.20",
    "sequelize-cli": "6.2.0",
    "sequelize-jest-helpers-trybe": "1.0.0",
    "supertest": "4.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4",
    "wait-on": "6.0.1"
  }
}